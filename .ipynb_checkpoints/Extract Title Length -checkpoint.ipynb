    "import textfeatures as tf\n",
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"Dataset.csv\",encoding=\"latin\")\n",
    "\n",
    "tf.word_count(df,\"Title\",\"word_cnt\")\n",
    "df[[\"Title\",\"word_cnt\"]]\n",
    "\n",
    "df.to_csv('Edited Dataset.csv')"
   ]
